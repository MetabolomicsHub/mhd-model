  "protocols": [
    {
      "name": "sample collection",
      "protocol_type": {
        "source": "EFO",
        "accession": "EFO:0005518",
        "name": "sample collection protocol"
      },
      "description": "All animal procedures were performed in accordance with a protocol approved by the Institutional Animal Care and Use Committee of the National Institute on Aging, NIH. Wildtype, homozygous and heterozygous E326K GBA1 knock-in (KI) male and female mice raised on a C57Bl/6 background were bred in-house on a 12-hr day/night cycle for the following experiments. All mice were supplied with Rodent NIH-07 diet and water ad libitum. The brain was then removed and one hemisphere retained without sub-dissection. Tissue samples were individually weighed, flash frozen on dry ice and stored at -80°C for subsequent analysis."
    },
    {
      "name": "sample preparation",
      "protocol_type": {
        "source": "MS",
        "accession": "MS:1000831",
        "name": "sample preparation"
      },
      "description": "20 (± 3) mg of brain tissues were weighed and flash frozen during tissue collection. Lipids were extracted from the frozen samples using 400 mL of HPLC-grade methanol (Fisher Chemical; # A452-4) containing stable-isotope internal standards and homogenized with a 3 mm tungsten carbide bead for 30 s at 25 Hz on TissueLyzer II (QIAGEN, #85300). The methanol fraction was isolated via centrifugation (20 min at 4°C, 21,000 x g, transferred to a 96-well sample collection plate (Waters; #186005837) and incubated for 1 h, at -20°C for protein precipitation, followed by an additional 20 min centrifugation (4,000 x g at 4°C). The supernatant was transferred to glass vial plates (Analytical systems, #27350) for LCMS analysis."
    },
    {
      "name": "treatment",
      "protocol_type": {
        "source": "EFO",
        "accession": "EFO:0003969",
        "name": "treatment protocol"
      },
      "description": "No treatment"
    },
    {
      "name": "mass spectrometry",
      "protocol_type": {
        "source": "CHMO",
        "accession": "CHMO:0000470",
        "name": "mass spectrometry"
      },
      "description": "curtain gas at 30 psi; collision gas set at medium; ion spray voltage at 4,500 V; temperature at 600°C; ion source Gas 1 at 55 psi; ion source Gas 2 at 60 psi."
    },
    {
      "name": "chromatography",
      "protocol_type": {
        "source": "CHMO",
        "accession": "CHMO:0001000",
        "name": "chromatography"
      }
    }
  ],
